#include <iostream>

using namespace std;

int main(int argc, char** argv) {
   
  if (argc==1) {
    cout<< "Not enough arguments";
  }
  if (argv[1]=="look") {
      argv[1]==1;
  } else if (argv[1]=="move") {
      argv[1]==2;
  } else if (argv[1]=="get") {
      argv[1]==3;
  } else if (argv[1]=="shout") {
      argv[1]==4;
  } else if (argv[1]=="help") {
      argv[1]==5;
  } else if (argv[1]=="logout") {
      argv[1]==6;
  }
  
   
    switch (argv[1]) {
        case 1:
            //Output all items in room
            break;
        case 2:
            //move to a room in direction argv[2]
            break;
        case 3:
            //get the item described in argv[2]
            break;
        case 4:
            //send the message in argv[2] to other players
            break;
        case 5:
            //send the player a list of commands
            break;
        case 6:
            //log the player out, if allowed
            break;
        
   }
   
}
